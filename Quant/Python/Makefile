	@ python3 -O BipartiteGeneralLindblad.py

bpgl_d:
	@ python3 BipartiteGeneralLindblad.py debug

clean_bpgl:
	@ sudo rm -r BipartiteGeneralLindblad/out/* 2>/dev/null || true
# -------------------------------------------------------------------------------------------------

