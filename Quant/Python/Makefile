# -------------------------------------------------------------------------------------------------
bp:
	@ python3 -O Bipartite.py

bp_d:
	@ python3 Bipartite.py debug

clean_bp:
	@ sudo rm -r Bipartite/out/* 2>/dev/null || true
# -------------------------------------------------------------------------------------------------
bp2:
	@ python3 -O Bipartite2.py

bp2_d:
	@ python3 Bipartite2.py
# -------------------------------------------------------------------------------------------------
# -------------------------------------------------------------------------------------------------
bpl:
	@ python3 -O BipartiteLindblad.py

bpl_d:
	@ python3 BipartiteLindblad.py debug

clean_bpl:
	@ sudo rm -r BipartiteLindblad/out/* 2>/dev/null || true
# -------------------------------------------------------------------------------------------------

# -------------------------------------------------------------------------------------------------
