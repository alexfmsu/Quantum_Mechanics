	@ python3 -O BipartiteGeneral.py

bpg_d:
	@ python3 BipartiteGeneral.py debug

clean_bpg:
	@ sudo rm -r BipartiteGeneral/out/* 2>/dev/null || true
# -------------------------------------------------------------------------------------------------
